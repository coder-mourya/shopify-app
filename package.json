  "_moduleAliases": {
    "utils": "src/utils",
    "services": "src/services",
    "dao": "src/dao",
    "types": "src/types",
    "config": "src/config",
    "controller": "src/controller",
    "middlewares": "src/middlewares",
    "validations": "src/validations",
    "routes": "src/routes",
    "constants": "src/constants",
    "swagger": "src/swagger"
  }
