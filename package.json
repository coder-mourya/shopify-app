{
  "name": "shopbolt",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc && npm run copy:assets",
    "copy:assets": "cpx \"src/**/*.json\" dist",
    "start": "node -r tsconfig-paths/register dist/app.js",
    "swagger": "npx ts-node -r tsconfig-paths/register src/swagger.ts ",
    "index": "nodemon index.js",
    "dev": "ts-node-dev --respawn --pretty --transpile-only -r tsconfig-paths/register src/app.ts",
    "prisma:migrate": "prisma migrate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "@shopify/shopify-api": "^11.13.0",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "joi-to-swagger": "^6.2.0",
    "jsonwebtoken": "^9.0.2",
    "localstack": "^1.0.0",
    "module-alias": "^2.2.3",
    "node-input-validator": "^4.5.1",
    "prisma": "^6.0.1",
    "querystring": "^0.2.1",
    "swagger-autogen": "^2.23.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/date-fns": "^2.5.3",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/mime-types": "^2.1.4",
    "@types/mock-aws-s3": "^2.6.3",
    "@types/module-alias": "^2.0.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@types/nodemailer": "^6.4.17",
    "@types/sanitize-html": "^2.15.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
